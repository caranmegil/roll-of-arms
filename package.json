    "v3-tour": "^3.0.16",
    "vue": "^3.2.36",
    "vue-loading-overlay": "^5.0.3",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@rollup/plugin-babel": "^5.3.1",
